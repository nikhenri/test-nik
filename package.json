{
	"name": "nikhenri",
	"displayName": "Nik Henri",
	"publisher": "NikHenri",
	"description": "Nik Henri vscode extension",
	"version": "2.0.6",
	"engines": {
		"vscode": "^1.56.0"
	},
	"icon": "camaro.jpg",
	"repository": {
		"type": "git",
		"url": "https://github.com/nikhenri/vscode_ext.git"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "systemverilog",
				"extensions": [
					".svh",
					".sv",
					".v"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "systemverilog",
				"scopeName": "source.systemverilog",
				"path": "./syntaxes/systemverilog.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {}
}
